{
  "name": "buildwave",
  "description": "Gerador de função: senoidal, triangular ou quadrada, com ajuste de frequência e amplitude.",
  "author": "Prof. Josué Morais - UFU",
  "version": "1.0.0",
  "files": [
    "main.c"
  ],
  "pins": [
    "OUT"
  ],
 "controls": [
    {
      "id": "waveform",
      "label": "Forma de onda",
      "type": "range",
      "min": 0,
      "max": 2,
      "step": 1,
      "default": 0
    },
    {
      "id": "frequency",
      "label": "Frequência (Hz)",
      "type": "range",
      "min": 1,
      "max": 1000,
      "step": 1,
      "default": 100
    },
    {
      "id": "amplitude",
      "label": "Amplitude (mV)",
      "type": "range",
      "min": 0,
      "max": 5000,
      "step": 100,
      "default": 1000
    }
  ]
}