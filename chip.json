{
  "name": "buildwave",
  "description": "Gerador de função: senoidal, triangular ou quadrada, com ajuste de frequência e amplitude.",
  "author": "Prof. Josué Morais - UFU",
  "version": 1,
  "pinCount": 1,
  "pins": [
    { "name": "OUT", "type": "analog", "direction": "output" }
  ],
  "attributes": {
    "freq1": { "type": "float", "default": 10.0 },
    "freq2": { "type": "float", "default": 110.0 },
    "freq3": { "type": "float", "default": 0.0 },
    "amp1": { "type": "float", "default": 1.0 },
    "amp2": { "type": "float", "default": 1.0 },
    "amp3": { "type": "float", "default": 1.0 }
  }
}
